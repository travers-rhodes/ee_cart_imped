<?xml version="1.0"?>
<!-- Load $(find ee_cart_imped_control)pr2_arms_cart_imped_controller parameters to the server 
     and then spawn the controller given by the newly uploaded parameter "r_arm_cart_imped_controller" from that file -->
<launch>
    <rosparam file="$(find ee_cart_imped_control)/pr2_arms_cart_imped_controller.yaml" command="load" />
    <node pkg="controller_manager" type="unspawner" args="r_arm_controller"  name="r_arm_stopper"/>
    <node pkg="controller_manager" type="spawner" args="r_arm_cart_imped_controller" name="r_arm_cart_imped_controller_spawner" />
</launch>
